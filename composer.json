{
    "name": "efabrica/version-history",
    "type": "library",
    "description": "PHP package template",
    "keywords": [
        "php",
        "package",
        "template"
    ],
    "readme": "README.md",
    "license": "proprietary",
    "authors": [
        {
            "name": "Martin Petricko",
            "email": "matopetricko@gmail.com"
        },
        {
            "name": "Michal Lulco",
            "email": "michal.lulco@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "../ndr-new"
        },
        {
            "type": "composer",
            "url": "https://composer.efabrica.sk"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "efabrica/nette-database-repository": "dev-class-behavior",
        "efabrica/nette-di-request-id-extension": "^1.0",
        "nette/security": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "Efabrica\\VersionHistory\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
